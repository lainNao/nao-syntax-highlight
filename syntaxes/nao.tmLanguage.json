{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "nao",
	"patterns": [
		{
			"include": "#titles"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"titles": {
			"patterns": [
				{
					"name": "markup.heading.2",
					"match": "■.*$"
				},
				{
					"name": "string.other",
					"match": "＜.*＞*"
				}		
			]
		},
		"comments": {
			"patterns": [{
				"name": "comment.line.double-slash",
				"match": "//.*$"
			}
			]
		}
	},
	"scopeName": "source.nao"
}